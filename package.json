    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "@dxcli/dev": "^0.0.2",
    "@dxcli/dev-lint": "^0.0.2",
    "@dxcli/dev-nyc-config": "^0.0.3",
    "@dxcli/dev-semantic-release": "^0.0.2",
    "@dxcli/dev-tslint": "^0.0.5",
    "@types/ansi-styles": "^2.0.30",
    "@types/chai": "^4.1.0",
    "@types/lodash": "^4.14.92",
    "@types/mocha": "^2.2.46",
    "@types/node": "^9.3.0",
    "@types/yeoman-generator": "^2.0.1",
    "@types/yosay": "^0.0.29",
    "chai": "^4.1.2",
    "del-cli": "^1.1.0",
    "ejs-lint": "^0.3.0",
    "eslint": "^4.15.0",
    "eslint-config-dxcli": "^1.1.2",
    "husky": "^0.14.3",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "yo": "^2.0.0"
  },
  "dxcli": {
    "workflows": {
      "test": [
        "eslint .",
        "tsc",
        "tslint -p .",
        "commitlint --from master",
        "yo dxcli basic tmp/test-basic --defaults && cd tmp/test-basic && git add . && git commit -m 'chore: init' && yarn test && yarn run lint"
      ],
      "lint": [
        "eslint .",
        "tsc",
        "tslint -p .",
        "commitlint --from master"
      ]
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/templates",
    "/generator"
  ],
  "keywords": [
    "dxcli",
    "yeoman-generator"
  ],
  "license": "MIT",
  "main": "lib/app/index.js",
  "repository": "dxcli/generator-dxcli",
  "scripts": {
    "commitmsg": "dxcli-dev-commitmsg",
    "lint": "dxcli-dev lint",
    "prepare": "del-cli generators && tsc",
    "test": "rm -rf tmp/test-basic && tsc && dxcli-dev test"
  }
}
